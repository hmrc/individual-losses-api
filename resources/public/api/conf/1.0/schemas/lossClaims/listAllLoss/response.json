{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "List All Loss Claims",
  "description": "List All Loss Claims",
  "type": "object",
  "properties": {
    "taxYear": {
      "type": "string",
      "description": "The tax year the data applies to, for example, 2018-19. The start year and end year must not span two tax years. No gaps are allowed, for example, 2017-19 is not valid. The minimum tax year is 2016-17.",
      "example": "2017-18"
    },
    "carryForward" : {
      "type" : "boolean",
      "description": "TBD.",
      "example": true
    },
    "carrySideways" : {
      "type" : "boolean",
      "description": "TBD.",
      "example": true
    },
    "carrySidewaysFhl" : {
      "type" : "boolean",
      "description": "TBD.",
      "example": true
    },
    "selfEmploymentId" : {
      "type": "string",
      "description": "An identifier for the self-employment business, unique to the customer.",
      "example": "XGIS00000001319",
      "pattern": "^X[A-Z0-9]{1}IS[0-9]{11}$"
    }
  },
  "required": ["taxYear","carryForward", "carrySideways", "carrySidewaysFhl"]
}